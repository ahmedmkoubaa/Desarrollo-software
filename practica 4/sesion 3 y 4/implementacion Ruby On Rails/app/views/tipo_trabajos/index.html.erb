<p id="notice"><%= notice %></p>

<h1>Tipo Trabajos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_trabajos.each do |tipo_trabajo| %>
      <tr>
        <td><%= tipo_trabajo.Tipo %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

